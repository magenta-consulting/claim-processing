{% spaceless %}
    {% if app.request.get('type') == 'checker-view-claim' %}
        <li><a class="sonata-action-element" href="{{ path('admin_app_claim_list',{'type':'checking-each-position','position-id':object.position.id}) }}"><i
                        class="fa fa-list" aria-hidden="true"></i>
                Return to list</a></li>
    {% else %}
        {% for item in admin.getActionButtons(action, object|default(null)) %}
            {% if item.template is defined %}
                <li>{% include item.template %}</li>
            {% endif %}
        {% endfor %}
    {% endif %}
{% endspaceless %}
